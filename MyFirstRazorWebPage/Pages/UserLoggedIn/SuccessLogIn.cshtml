@page
@model MyFirstRazorWebPage.Pages.UserLoggedIn.SuccessLogInModel


@{
    ViewData["Title"] = "User Page";
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>User Page</title>
</head>

<body>

    <h1 style="text-align:center">Welcome @Model.UserName</h1>

    <div>
        <a asp-page="/UserModules/UserProfile">View/Update Profile</a>|
        <a asp-page="/Users/UserLogin" asp-page-handler="OnGetLogout">View Registered Modules</a>|
        <a asp-page="/UserModules/RegisterModules">Register Modules</a>|
        <a asp-page="/Users/UserLogin" asp-page-handler="OnGetLogout">Logout</a>
    </div>

</body>

</html>